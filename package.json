{
  "name": "metalsthith-blog-source",
  "version": "1.0.0",
  "description": "My blog using metalsmith static generator.",
  "main": "metalsmith.js",
  "private": true,
  "scripts": {
    "start": "gulp && gulp serve:dev",
    "build": "gulp",
    "deploy": "NODE_ENV=production gulp deploy",
    "sitemap": "gulp seo",
    "tunel": "gulp tunel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaliy-bobrov/metalsmith-blog-source.git"
  },
  "author": "Vitaliy Bobrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitaliy-bobrov/metalsmith-blog-source/issues"
  },
  "homepage": "https://github.com/vitaliy-bobrov/metalsmith-blog-source#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-preset-es2015": "^6.22.0",
    "browser-sync": "^2.18.5",
    "css-mqpacker": "^5.0.1",
    "del": "^2.2.2",
    "escape-html": "^1.0.3",
    "eslint-config-google": "^0.7.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.2.0",
    "gulp-sass": "^3.0.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-uglify": "^2.0.1",
    "gulp-uncss": "^1.0.6",
    "gulp-webp": "^2.3.0",
    "gulp-webpcss": "^1.1.1",
    "handlebars": "^4.0.6",
    "helper-moment": "0.1.0",
    "metalsmith": "^2.3.0",
    "metalsmith-author": "^1.1.0",
    "metalsmith-code-highlight": "0.0.3",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-debug": "^1.1.0",
    "metalsmith-default-values": "^1.0.1",
    "metalsmith-disqus": "^1.2.2",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-excerpts": "^1.1.0",
    "metalsmith-feed": "^0.2.0",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-mapsite": "^1.0.4",
    "metalsmith-markdown-remarkable": "^0.1.1",
    "metalsmith-pagination": "^1.4.0",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-register-helpers": "^0.2.0",
    "metalsmith-twitter-card": "^1.0.2",
    "metalsmith-updated": "^0.1.4",
    "minimist": "^1.2.0",
    "ngrok": "^2.2.5",
    "postcss-assets": "^4.1.0",
    "postcss-mq-keyframes": "^0.3.0",
    "remarkable-classy": "0.0.1",
    "remarkable-extlink": "^1.0.0",
    "run-sequence": "^1.2.2",
    "sw-precache": "^4.3.0",
    "sw-toolbox": "^3.4.0"
  },
  "dependencies": {
    "material-design-lite": "^1.3.0",
    "mdl-ext": "^0.15.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "info",
            "warn",
            "error"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    }
  }
}
